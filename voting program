#include<iostream>
using namespace std;
int main()
{
	int age; 
	cout<<"Enter the age :";
	cin>>age;
	
	//person is teenager or not
	//>=13 and <=19
	if(age>=13 &&age>=19)
	{
		cout<<"Person is teenager"<<endl;
	}
	else
	{
		cout<<"Person is not teenager"<<endl;
	}
	
	//to check person is eligible for vote or not
	if(age>=18)
	{
		cout<<"Person is eligible for vote"<<endl;
	}
	else
	{
	    cout<<"Person  is not eligible for vote"<<endl;
	}
	return 0;
}
	
